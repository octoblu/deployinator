
[info]
  travis:       {{{ context.status.travis }}}
  quay:         {{{ context.status.quay }}}
  majorVersion: {{{ context.majorVersion }}}
  minorVersion: {{{ context.minorVersion }}}

[deployments]
{{#context.deployments}}
  {{{ key }}}
    deployAt: {{{ deployAt }}}
    status:   {{{ status }}}
{{/context.deployments}}

[servers]
{{#context.servers}}
  {{{ name }}}: {{{ url }}}
{{/context.servers}}
